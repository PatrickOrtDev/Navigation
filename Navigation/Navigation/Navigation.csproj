<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>POD.Navigation</PackageId>
    <Authors>Patrick Ort Dev</Authors>
    <Description>A Navigation service that allows navigating to ViewModels.</Description>
    <PackageTags>navigation c# wpf mvvm</PackageTags>
    <PackageProjectUrl>https://github.com/PatrickOrtDev/Navigation</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PatrickOrtDev/Navigation</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Interfaces\IModalNavigationService.cs" />
    <Compile Remove="Interfaces\IModalNavigationStore.cs" />
    <Compile Remove="Services\ModalNavigationService.cs" />
    <Compile Remove="Stores\ModalNavigationStore.cs" />
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="LightInject" Version="6.6.1" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="6.0.0" />
  </ItemGroup>

</Project>
